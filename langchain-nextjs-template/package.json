{
  "name": "doctor-gpt",
  "version": "1.0.0",
  "description": "Advanced medical AI assistant with multi-model reasoning, RAG, and healthcare focus",
  "private": true,
  "packageManager": "bun@1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"app\"",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:seed": "bun run scripts/seed-medical-data.ts",
    "db:init": "bun run scripts/init-databases.ts",
    "db:setup": "bun run db:generate && bun run db:migrate && bun run db:init",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:dev": "docker-compose --profile dev up -d",
    "docker:logs": "docker-compose logs -f",
    "validate-config": "bun run -e \"import('./config/index.js').then(m => m.config.validateConfig())\"",
    "cost-report": "bun run scripts/generate-cost-report.ts",
    "health-check": "bun run scripts/health-check.ts"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@langchain/anthropic": "^0.3.27",
    "@langchain/community": "^0.3.37",
    "@langchain/core": "^0.3.43",
    "@langchain/langgraph": "^0.2.57",
    "@langchain/langgraph-sdk": "^0.0.60",
    "@langchain/openai": "^0.4.9",
    "@prisma/client": "^5.20.0",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/supabase-js": "^2.32.0",
    "@types/node": "20.12.12",
    "@types/react": "18.3.2",
    "@types/react-dom": "18.3.0",
    "ai": "^3.1.12",
    "buffer": "^6.0.3",
    "canvas": "^3.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "form-data": "^4.0.4",
    "langchain": "^0.3.19",
    "lucide-react": "^0.473.0",
    "next": "^15.2.4",
    "next-themes": "^0.4.4",
    "node-fetch": "^3.3.2",
    "nuqs": "^2.3.2",
    "pdf-parse": "^1.1.1",
    "pdf-poppler": "^0.2.1",
    "pdf2pic": "^3.2.0",
    "pdfjs-dist": "^5.4.149",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-toastify": "^9.1.3",
    "sharp": "^0.34.3",
    "sonner": "^1.7.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "tesseract.js": "^6.0.1",
    "use-stick-to-bottom": "^1.0.44",
    "uuid": "^10.0.0",
    "vaul": "^1.1.2",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.4.19",
    "@qdrant/js-client-rest": "^1.15.1",
    "@types/uuid": "^9.0.0",
    "autoprefixer": "10.4.14",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.12",
    "postcss": "8.4.27",
    "prettier": "^3.4.2",
    "prisma": "^5.20.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6"
  },
  "keywords": [
    "medical-ai",
    "healthcare",
    "langchain",
    "openai",
    "anthropic",
    "vector-database",
    "rag",
    "multi-model",
    "nextjs",
    "typescript"
  ],
  "author": "Doctor GPT Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/langchain-ai/langchain-nextjs-template"
  },
  "bugs": {
    "url": "https://github.com/langchain-ai/langchain-nextjs-template/issues"
  },
  "homepage": "https://github.com/langchain-ai/langchain-nextjs-template#readme"
}
